    "import yfinance as yf\n",
    "\n",
    "# Create a ticker object\n",
    "ticker = yf.Ticker(\"AAPL\")  # Apple Inc.\n",
    "\n",
    "# Get stock info\n",
    "info = ticker.info\n",
    "print(info[\"currentPrice\"])\n",
    "\n",
    "# Get historical data (last 1 month)\n",
    "hist = ticker.history(period=\"1mo\")\n",
    "print(hist)"
   ]
